{
  "name": "badgen.net",
  "description": "Badge generating service",
  "author": "Amio <amio.cn@gmail.com>",
  "license": "ISC",
  "private": true,
  "scripts": {
    "lint": "standard",
    "build": "npm run tools && next build && next export -o dist && tsc",
    "dev": "npm run build && npm run dev:api",
    "dev:api": "nodemon --config nodemon.json index.ts",
    "dev:web": "next",
    "tools": "ts-node tools/gen-examples.ts",
    "now-build": "npm run build",
    "pretest": "npm run lint",
    "test": "tap test/*.js --reporter spec -j12",
    "start": "node dist/index.js"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "@sentry/integrations": "^5.4.1",
    "@sentry/node": "^5.4.0",
    "badgen": "^2.7.1",
    "badgen-icons": "^0.9.1",
    "byte-size": "^5.0.1",
    "cheerio": "^1.0.0-rc.3",
    "chrome-webstore": "^1.2.1",
    "date-fns": "^1.30.1",
    "got": "^9.6.0",
    "lodash.debounce": "^4.0.8",
    "micro": "^9.3.4",
    "millify": "^3.1.0",
    "react-debounce-render": "^5.0.0",
    "semver": "^6.1.0",
    "serve-handler": "^6.0.1",
    "serve-marked": "^2.0.0"
  },
  "devDependencies": {
    "@types/got": "^9.4.4",
    "@zeit/next-typescript": "^1.1.1",
    "babel-eslint": "^10.0.1",
    "fs-extra": "^8.0.1",
    "next": "^7.0.3",
    "nodemon": "^1.19.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "standard": "^12.0.1",
    "supertest": "^4.0.2",
    "tap": "^13.1.11",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  }
}
